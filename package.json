{
  "name": "meldio",
  "version": "0.4.7",
  "description": "Open Source GraphQL Backend For Building Delightful Mobile and Web Apps",
  "license": "AGPL-3.0",
  "author": {
    "name": "Nemanja Stanarevic",
    "email": "nemanja@meldio.com"
  },
  "main": "./dist/index.js",
  "bin": {
    "meldio": "./dist/meldio.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/meldio/meldio.git"
  },
  "bugs": {
    "url" : "https://github.com/meldio/meldio/issues",
    "email" : "support@meldio.com"
  },
  "homepage": "https://www.meldio.com",
  "keywords": [
    "graphql",
    "relay",
    "react",
    "meldio",
    "backend",
    "mongodb"
  ],
  "options": {
    "mocha": "--require resources/mocha-bootload src/**/__tests__/**/*.js -t 5000"
  },
  "babel": {
    "optional": [
      "runtime"
    ]
  },
  "scripts": {
    "start": "babel-node src/index.js",
    "test": "npm run lint && npm run check && npm run testonly",
    "testonly": "mocha $npm_package_options_mocha",
    "lint": "eslint src",
    "check": "flow check",
    "build": "babel src --optional runtime --ignore __tests__ --out-dir dist/ && cp package.json dist/ && cp ./src/cli/run/static/* ./dist/cli/run/static/",
    "watch": "babel --optional runtime resources/watch.js | node",
    "cover": "babel-node node_modules/.bin/isparta cover --root src --report html node_modules/.bin/_mocha -- $npm_package_options_mocha",
    "cover:lcov": "babel-node node_modules/.bin/isparta cover --root src --report lcovonly node_modules/.bin/_mocha -- $npm_package_options_mocha",
    "preversion": ". ./resources/checkgit.sh && npm test",
    "prepublish": ". ./resources/prepublish.sh"
  },
  "dependencies": {
    "ansi-256-colors": "^1.1.0",
    "babel": "^5.8.x",
    "babel-code-frame": "^6.6.x",
    "babel-core": "^5.8.x",
    "babel-eslint": "^4.1.7",
    "babel-runtime": "^5.8.x",
    "bcrypt": "^0.8.5",
    "body-parser": "^1.15.0",
    "chalk": "^1.1.1",
    "chokidar": "^1.4.3",
    "commander": "^2.9.0",
    "content-type": "~1.0.1",
    "cors": "^2.7.1",
    "eslint": "^1.10.1",
    "eslint-plugin-babel": "^2.1.1",
    "express": "^4.13.4",
    "express-graphql": "^0.4.9",
    "fbjs": "^0.7.2",
    "form-data": "^1.0.0-rc3",
    "fs-cp": "^1.4.0",
    "glob": "^7.0.0",
    "graphql": "^0.4.18",
    "http-errors": "~1.3.1",
    "inquirer": "^0.12.0",
    "jsonwebtoken": "^5.7.0",
    "jwa": "^1.1.3",
    "lodash": "^3.10.0",
    "mkdirp": "^0.5.1",
    "mongodb": "^2.1.7",
    "progress": "^1.1.8",
    "qs": "^6.1.0",
    "raw-body": "~2.1.2",
    "repeating": "^2.0.0",
    "rimraf": "^2.5.2",
    "semver": "^5.1.0"
  },
  "devDependencies": {
    "bluebird": "^3.3.3",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.2.0",
    "chai-subset": "^1.2.1",
    "coveralls": "^2.11.8",
    "flow-bin": "^0.22.0",
    "isparta": "^3.5.3",
    "minimist": "^1.2.0",
    "mocha": "^2.4.5",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "express3": "*",
    "multer": "1.0.3",
    "sane": "^1.3.3",
    "supertest": "1.0.1",
    "supertest-as-promised": "2.0.2"
  }
}
